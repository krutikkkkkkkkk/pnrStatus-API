{
    "version" :2,
    "builds": [
        {
            "src": "./main.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/",
            "dest": "./index.html"
        },
        {
            "src": "/api(.*)",
            "dest": "/api"
        }
    ]

}
